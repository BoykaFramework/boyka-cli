{
  "$schema": "https://unpkg.com/release-it@17/schema/release-it.json",
  "plugins": {
    "release-it-pnpm": {
      "disableRelease": true
    }
  },
  "git": {
    "commit": false,
    "tag": false,
    "push": false
  },
  "github": false,
  "npm": {
    "publish": true,
    "skipChecks": true,
    "publishPath": "dist"
  }
}
